{
	"name": "@infinityfx/fluid",
	"version": "0.0.1",
	"description": "React UI library.",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"types": "./types/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		},
		"./utils": {
			"import": "./dist/esm/utils.js",
			"require": "./dist/cjs/utils.js"
		}
	},
	"typesVersions": {
		"*": {
			"utils": [
				"./types/utils.d.ts"
			]
		}
	},
	"repository": "infinityfx-llc/fluid",
	"author": "InfinityFX <contact@infinity-fx.com> (https://infinity-fx.com)",
	"license": "GPL-3.0",
	"bugs": "https://github.com/infinityfx-llc/fluid/issues",
	"homepage": "https://github.com/infinityfx-llc/fluid#readme",
	"funding": "https://ko-fi.com/infinityfx",
	"keywords": [
		"react",
		"ui"
	],
	"scripts": {
		"build": "rollup -c --environment NODE_ENV:production",
		"watch": "rollup -c -w"
	},
	"peerDependencies": {
		"@babel/runtime": "^7.19.0",
		"@infinityfx/lively": "^2.1.6",
		"react": ">=16.14.0",
		"react-dom": ">=16.14.0"
	},
	"sideEffects": false
}
